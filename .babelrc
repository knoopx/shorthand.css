  "presets": [
    "react",
    ["latest", {
      "es2015": {
        "loose": true,
        "modules": false,
      },
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
